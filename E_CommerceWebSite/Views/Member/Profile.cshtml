
@{
    ViewBag.Title = "Profile";
    Layout = "~/Views/Shared/Home_Layout.cshtml";
}
@model E_CommerceWebSite.Models.Client
<!DOCTYPE html>
<html lang="en">

<head>
  
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Mosaddek">
    <meta name="keyword" content="FlatLab, Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <link rel="shortcut icon" href="/Content/Admin/img/favicon.png">

    <title>Profile</title>

    <!-- Bootstrap core CSS -->
    <link href="/Content/Admin/css/bootstrap.min.css" rel="stylesheet">
    <link href="/Content/Admin/css/bootstrap-reset.css" rel="stylesheet">
    <!--external css-->
    <link href="/Content/Admin/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="/Content/Admin/css/style.css" rel="stylesheet">
    <link href="/Content/Admin/css/style-responsive.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
    <!--[if lt IE 9]>
      <script src="/Content/Admin/js/html5shiv.js"></script>
      <script src="/Content/Admin/js/respond.min.js"></script>
    <![endif]-->
</head>

<body>
                <!-- page start-->
                <div class="row">
                    <aside class="profile-nav col-lg-3">
                        <section class="panel">
                            <div class="user-heading round">
                                <a href="/Content/Admin/#">
                                    <img src="/Content/Admin/img/profile-avatar.jpg" alt="">
                                </a>
                                <h1>@User.Identity.Name</h1>
                                <p></p>
                            </div>

                            <ul class="nav nav-pills nav-stacked">
                                <li class="active"><a href="/Member/Profile"> <i class="icon-user"></i> Profile</a></li>
                                <li><a href="/Member/AddClient"> <i class="icon-edit"></i> Edit profile</a></li>
                            </ul>

                        </section>
                    </aside>
                    <aside class="profile-info col-lg-9">

                        <section class="panel">

                            <div class="panel-body bio-graph-info">
                                <h1>Bio Graph</h1>
                                <div class="row">
                                    <div class="bio-row">
                                        <p><span>First Name </span>: @Model.Name</p>
                                    </div>
                                    <div class="bio-row">
                                        <p><span>Last Name </span>: @Model.Surname</p>
                                    </div>
                                    <div class="bio-row">
                                        <p><span>Email </span>:@Model.Email</p>
                                    </div>
                                    <div class="bio-row">
                                        <p><span>Mobile </span>: @Model.Phone</p>
                                    </div>
                                    <div class="bio-row">
                                        <p><span>Adress </span>: @ViewBag.Adress.Adress</p>
                                    </div>

                                </div>
                            </div>
                        </section>

                    </aside>
                </div>

                <!-- page end-->

    <!-- js placed at the end of the document so the pages load faster -->
    <script src="/Content/Admin/js/jquery.js"></script>
    <script src="/Content/Admin/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="/Content/Admin/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="/Content/Admin/js/jquery.scrollTo.min.js"></script>
    <script src="/Content/Admin/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="/Content/Admin/assets/jquery-knob/js/jquery.knob.js"></script>
    <script src="/Content/Admin/js/respond.min.js"></script>

    <!--common script for all pages-->
    <script src="/Content/Admin/js/common-scripts.js"></script>




</body>
</html>



